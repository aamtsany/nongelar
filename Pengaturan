	public function import()
	{
		$config['upload_path']      = 'D:\\file/';
		$config['allowed_types']    = 'xls|xlsx';
		$config['file_name']        = 'temp';
		$config['overwrite']        = TRUE;
		$config['max_size']         = '12024';

		$this->load->library('upload', $config);

		if ( ! $this->upload->do_upload('files'))
		{
			$error = array('error' => $this->upload->display_errors());
			print_r( $error );
		}
		else
		{
			$uploadData = $this->upload->data();
		}

		$this->load->library('excel');
		if($this->_importtemp($uploadData['full_path'])){
			redirect('ppk/pembiayaan');
		}else{
			echo 'Data gagal diimport. Silahkan coba lagi.';
		}

		//$data['file']			= $uploadData['full_path'];
		//$data['title'] 			= 'Excel To PHP';
		//$data['head'] 			= $this->load->view('head/head', $data, TRUE);
		// $data['k'] 			= $this->load->view('user/cetak-bukti-daftar', $data, TRUE);
	  //$this->load->view('admin/admin/import/import_soal', $data);
	}

	function _importtemp($file)
	{
		//$file = 'uploads/'.$file;

		$this->load->library('excel');

		$objPHPExcel = PHPExcel_IOFactory::load($file);

		$cell_collection = $objPHPExcel->getActiveSheet()->getCellCollection();

		foreach ($cell_collection as $cell) {
			$column = $objPHPExcel->getActiveSheet()->getCell($cell)->getColumn();
			$row = $objPHPExcel->getActiveSheet()->getCell($cell)->getRow();
			$data_value = $objPHPExcel->getActiveSheet()->getCell($cell)->getValue();

			if ($row > 1) {
				$arr_data[$row][$column] = $data_value;
			}
		}

		//$data['header'] = $header;
		$data['values'] = $arr_data;

		$values	= $arr_data;

		foreach($values as $isi){
			$soal[]= array(
				'id_peserta' => $isi['A'],
				'no_sp2d' => $isi['B'],
				'tgl_pembayaran' => date('Y-m-d', PHPExcel_Shared_Date::ExcelToPHP($isi['C'])),
				'pembayaran' => $isi['D'],
				'rupiah' => $isi['E'],
				'kurs' => $isi['F'],
				'yen' => $isi['G'],
				'ket_pembayaran' => $isi['H'],
				'keterangan' => $isi['I']
			);
		}

		$insert	= $this->Pembiayaan_model->import($soal);

		if($insert){
			return TRUE;
		}else{
			return FALSE;
		}

	}
	function data_peserta() {
		$data = array();
		$peserta = $this->Pembiayaan_model->get_peserta();
		if(!empty($peserta)) {
			foreach($peserta as $pes) {
				array_push($data, array(
					'id' => $pes->id_peserta,
					'text' => $pes->nip.' - '.$pes->nama
				));
			}
		}
		echo json_encode(array('results'=>$data));
	}
}
