		$tujuan				= $this->input->get('tujuan');
		$output				= $this->input->get('output');
		// $			= $this->input->get('');
		// $			= $this->input->get('');
		// $			= $this->input->get('');
		// $			= $this->input->get('');
		// $instansi			= $this->input->get('instansi');
