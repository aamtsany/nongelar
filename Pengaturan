	function add(){
		// print_r($_POST);
		// exit;
		date_default_timezone_set("Asia/Bangkok");
		$dataLast = $this->Non_gelar_model->getLastID();
		// echo $dataLast[0]->id_datadiri;
		$nomorID = substr($dataLast[0]->id_datadiri, 1);
		$nomorBaru = null;
		$nomorBaru = $nomorID+1;
		$digitNomor = strlen($nomorBaru);
		// echo "AWAL ".$digitNomor;
		for ($i=$digitNomor; $i < 9; $i=$i+1) { 
			// if ($digitNomor < ) {
			// 	# code...
			// }
			$nomorBaru = "0".$nomorBaru;
		}
		$fixID = 'Z'.($nomorBaru);
		// echo $fixID;
		// die();
		
		$data['nip']			= $this->input->post('nip');
		$data['nama'] 			= $this->input->post('nama');
		$data['gender'] 		= $this->input->post('gender');
		$data['tgl_lahir'] 		= $this->input->post('tgl_lahir');
		$data['tmp_lahir'] 		= $this->input->post('tmp_lahir');
		$data['alamat_rmh'] 	= $this->input->post('alamat_rmh');
		$data['prop_rmh'] 		= $this->input->post('prop_rmh');
		$data['kabkot_rmh'] 	= $this->input->post('kabkot_rmh');
		$data['kodepos_rmh'] 	= $this->input->post('kodepos_rmh');
		$data['telp_rmh'] 		= $this->input->post('telp_rmh');
		$data['hp'] 			= $this->input->post('hp');
		$data['email'] 			= $this->input->post('email');
		$data['wilayah'] 		= $this->input->post('wilayah');
		$data['nama_inst'] 		= $this->input->post('nama_inst');
		$data['unit_kerja'] 	= $this->input->post('unit_kerja');
		$data['almt_inst'] 		= $this->input->post('almt_inst');
		$data['kodepos_inst'] 	= $this->input->post('kodepos_inst');
		$data['telp_inst'] 		= $this->input->post('telp_inst');
		$data['fax'] 			= $this->input->post('fax');
		$data['wilayah'] 		= $this->input->post('wilayah');
		$data['tgl_pns'] 		= $this->input->post('tgl_pns');
		$data['dokskpns']		= $this->input->post('dokskpns');
		$data['jabatan']		= $this->input->post('jabatan');
		$data['dokjabatan']		= $this->input->post('dokjabatan');
		$data['bagian']			= $this->input->post('bagian');
		$data['pnddkn']			= $this->input->post('pnddkn');
		$data['dokijazah']		= $this->input->post('dokijazah');
		$data['pt']				= $this->input->post('pt');
		$data['fakultas']		= $this->input->post('fakultas');
		$data['jurusan']		= $this->input->post('jurusan');
		$data['thn_lulus']		= $this->input->post('thn_lulus');
		$data['ipk']			= $this->input->post('ipk');
		$data['doktranskip']	= $this->input->post('doktranskip');
		$data['wilayah']		= $this->input->post('wilayah');
		$data['urutan_ngds']	= $this->input->post('urutan_ngds');
		$data['pil1_ngds']		= $this->input->post('pil1_ngds');	
		$data['pil2_ngds']		= $this->input->post('pil2_ngds');
		$data['pekerjaan']		= $this->input->post('pekerjaan');
		$data['latarbelakang']	= $this->input->post('latarbelakang');
		$data['tujuan']			= $this->input->post('tujuan');
		$data['output']			= $this->input->post('output');

		$data['tgl_akun_dibuat']= date('Y-m-d h:i:s');
		$data['id_datadiri'] 	= $fixID;

		// print_r($data);
		// exit;

		if($this->Non_gelar_model->insert($data)){
			$this->session->set_flashdata('success','Data pembiayaan berhasil di tambah');
		}else{
			$this->session->set_flashdata('danger','Tambah data tidak berhasil');
		}

		redirect('ppk/pengaturan/non_gelar','refresh');
	}


	public function update_pendaftaran($id='')
	{}
	/*{
		$data['title']      = 'Data Kelulusan';
